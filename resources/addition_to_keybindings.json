// add this into your vscode keybindings.json
{
  "key": "ctrl+shift+s",
  "command": "extension.multiCommand.execute",
  "args": {
    "sequence": [
      "workbench.action.files.save",
      "markdown.extension.printToHtml",
      {
        "command": "command-runner.run",
        "args": {
          "command": "./resources/html_to_pdf.sh"
        }
      }
    ]
  },
  "when": "config.workspaceKeybindings.pdfPipeline.enabled && editorLangId == markdown && resourceFilename == resume.md"
}
